{
    "name": "swoft/http-client",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "http-client",
        "http",
        "client"
    ],
    "description": "HTTP Client Component for Swoft",
    "license": "Apache-2.0",
    "require": {
        "swoft/framework": "dev-component",
        "swoft/http-message": "dev-master",
        "swoft/http-server": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\Http\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/swoft-cloud/framework"
        },
        {
            "type": "vcs",
            "url": "https://github.com/swoft-cloud/swoft-http-message"
        },
        {
            "type": "vcs",
            "url": "https://github.com/swoft-cloud/swoft-http-server"
        },
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}
